---
import type { GetStaticPaths } from 'astro'
import { Image } from 'astro:assets'
// 导入图片
import BaseLayout from '@/layouts/BaseLayout.astro'
import lineIcon from './static/line.png'

export const getStaticPaths = (() => {
  return [
    { params: { people: '藤野英人', version: '0', img_prefix: 'tyyr_' } },
  ]
}) satisfies GetStaticPaths

const { people, version = '0' } = Astro.params
const img_prefix = getStaticPaths().find(
  path => path.params.people === people && path.params.version === version,
)?.params.img_prefix
const img1 = import(`./static/${img_prefix}img1.jpg`)
const img2 = import(`./static/${img_prefix}img2.jpg`)
const img3 = import(`./static/${img_prefix}img3.jpg`)
const img4 = import(`./static/${img_prefix}img4.jpg`)
---

<BaseLayout title="株式市場情報です" plugins={{ aos: {}, jquery: {} }}>

</BaseLayout>
