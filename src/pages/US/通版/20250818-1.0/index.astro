                      <h4>Latest Stock Alerts</h4>
                      <div class="box-stock-row">
                        <div class="box-inner fn">
                          <div class="box-hd">
                            FNMA
                            <p>ALERTED:$0.071</p>
                            <p>JUMPED:$1.136</p>
                          </div>
                          <div class="box-ft">
                            <h5><i class="fa-solid fa-arrow-up"></i> 1600%</h5>
                          </div>
                          <img
                            decoding="async"
                            class="fn-img"
                            src={breakoutImg.src}
                            alt=""
                          />
                        </div>

                        <div class="box-inner cr">
                          <div class="box-hd">
                            CGRW
                            <p>ALERTED:$0.013</p>
                            <p>JUMPED:$0.0658</p>
                          </div>
                          <div class="box-ft">
                            <h5><i class="fa-solid fa-arrow-up"></i> 506%</h5>
                          </div>
                          <img
                            decoding="async"
                            class="cr-img"
                            src={marketImg.src}
                            alt=""
                          />
                        </div>

                        <div class="box-inner thz">
                          <div class="box-hd">
                            THCZ
                            <p>ALERTED:$0.521</p>
                            <p>JUMPED:$1.214</p>
                          </div>
                          <div class="box-ft">
                            <h5><i class="fa-solid fa-arrow-up"></i> 233%</h5>
                          </div>
                          <img
                            decoding="async"
                            class="thz-img"
                            src={breakoutImg.src}
                            alt=""
                          />
                        </div>

                        <div class="box-inner btl">
                          <div class="box-hd">
                            BTLLF
                            <p>ALERTED:$3.111</p>
                            <p>JUMPED:$6.235</p>
                          </div>
                          <div class="box-ft">
                            <h5><i class="fa-solid fa-arrow-up"></i> 201%</h5>
                          </div>
                          <img
                            decoding="async"
                            class="btl-img"
                            src={marketImg.src}
                            alt=""
                          />
                        </div>
                      </div>
                    </div>
                  </section>
                </div>
              </div>
            </section>

            <!-- Leading Section -->
            <section class="leading-sec">
              <div class="container">
                <div class="grid-leading">
                  <div class="left-leading">
                    <section id="block-12" class="widget widget_block">
                      <h3>
                        Our<br />
                        <span>"Track Record is our"</span><br />
                        Reputation
                      </h3>
                      <p id="counters-nm" class="number-sp">
                        <span
                          class="bl-text-nu counter-value"
                          data-count="8"
                        >0</span
                        ><b>/10</b>
                      </p>
                      <span class="bg-sp">Eight out of our last ten Stock Alerts rallied for Huge
                        Gains</span
                      >
                    </section>
                  </div>

                  <div class="right-leading">
                    <h3>Real Traders. Real Results.</h3>

                    <!-- Testimonials Slider -->
                    <div class="client-slider">
                      <section id="block-2" class="widget widget_block">
                        <div
                          class="rt-container-fluid tss-wrapper"
                          id="tss-container-109931952"
                          data-layout="carousel3"
                          data-desktop-col="1"
                          data-tab-col="1"
                          data-mobile-col="1"
                        >
                          <div
                            data-title="Loading ..."
                            class="rt-row tss-carousel3 tss-even tss-pre-loader"
                          >
                            <div class="carousel-wrapper">
                              <div
                                dir="rtl"
                                class="tss-carousel swiper"
                                data-loop="true"
                                data-items-desktop="1"
                                data-items-tab="1"
                                data-items-mobile="1"
                                data-autoplay="true"
                                data-autoplay-timeout="5000"
                                data-autoplay-hover-pause="true"
                                data-dots="true"
                                data-nav="true"
                                data-lazy-load="true"
                                data-auto-height="true"
                                data-smart-speed="250"
                              >
                                <div class="swiper-wrapper">
                                  <div
                                    class="rt-col-md-12 rt-col-sm-12 rt-col-xs-12 even-grid-item tss-grid-item slide-item swiper-slide even-grid-item default-margin"
