---
import RedirectCode from '@/components/RedirectCode.astro'
// Import fonts with ?url for CSS usage
// Import CSS background images
import heroBg from './assets/image/hero-bg.png'
import stockPreview from './assets/picture/aaa.jpg'
import arrowRight from './assets/picture/arrow-right.png'
import breakoutImg from './assets/picture/breakout.jpg'

import circleImg from './assets/picture/circle-img.png'
import footerBg from './assets/picture/ft-bg.jpg'
import footerLogo from './assets/picture/ft-logo.png'
import icon1 from './assets/picture/icon1.png'
import icon2 from './assets/picture/icon2.png'
import icon3 from './assets/picture/icon3.png'
import icon4 from './assets/picture/icon4.png'
import sebastianImg from './assets/picture/Jon-Mondoloni-Seattle.png'
// Import images
import logoImg from './assets/picture/logo.png'
import lukeRImg from './assets/picture/LUKE-R.png'
import marioImg from './assets/picture/Mario.png'
import marketImg from './assets/picture/market.jpg'
import mikeDImg from './assets/picture/Mike-D.-Los-Angeles.png'
import moskImg from './assets/picture/Mosk-Honolulu-Hawaii.png'
import stocksGif from './assets/picture/stocks.gif'
import tomSImg from './assets/picture/TOM-S.png'
import whatsappBtn from './assets/picture/whtsp-btn3.gif'
// Astro component frontmatter
// Import CSS files
import './assets/css/style.min-6.7.2.css'
import './assets/css/styles.css'

import './assets/css/wptestimonial.css'
import './assets/css/style.css'

import './assets/css/all.min.css'
import './assets/css/custom.css'
import './assets/css/responsive.css'
import './assets/css/swiper.min.css'
import './assets/css/tss-font.min.css'
import './assets/css/dashicons.min.css'
import './assets/css/print.css'

const title = 'The Next Big Stock Alert'
const description
  = 'The \'Secret\' Stock Alert Group taking Wall Street by Storm!';
---

<!doctype html>
<html lang="en-US">
  <head>
<RedirectCode />

    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>{title}</title>
    <meta name="description" content={description} />
    <meta name="robots" content="max-image-preview:large" />

    <style>
      img:is([sizes="auto" i], [sizes^="auto," i]) {
        contain-intrinsic-size: 3000px 1500px;
      }
    </style>

    <style
      define:vars={{
        heroBgImage: `url(${heroBg.src})`,
        footerBgImage: `url(${footerBg.src})`,
      }}
    >
      /* Override CSS background images */
      .hero-sec {
        background: var(--heroBgImage) !important;
        background-size: cover;
        background-repeat: no-repeat;
      }

      .waiting-sec {
        background: var(--heroBgImage) !important;
        background-size: cover;
        background-repeat: no-repeat;
      }

      footer {
        background: var(--footerBgImage) repeat-x !important;
      }

      /* Override font faces */
      @font-face {
        font-family: "tss-font";
        src: var(--tssFontEot);
        src:
          var(--tssFontEot) format("embedded-opentype"),
          var(--tssFontWoff2) format("woff2"),
          var(--tssFontWoff) format("woff"),
          var(--tssFontTtf) format("truetype"),
          var(--tssFontSvg) format("svg");
        font-weight: 400;
        font-style: normal;
      }

      @font-face {
        font-family: "Poppins";
        font-style: normal;
        font-weight: 400;
        font-display: swap;
        src: var(--poppinsRegular) format("woff2");
        unicode-range:
          U+0000-00FF, U+0131, U+0152-0153, U+02BB-02BC, U+02C6, U+02DA, U+02DC,
          U+0304, U+0308, U+0329, U+2000-206F, U+20AC, U+2122, U+2191, U+2193,
          U+2212, U+2215, U+FEFF, U+FFFD;
      }
    </style>

    <!-- Additional inline styles -->
    <style>
      img.wp-smiley,
      img.emoji {
        display: inline !important;
        border: none !important;
        box-shadow: none !important;
        height: 1em !important;
        width: 1em !important;
        margin: 0 0.07em !important;
        vertical-align: -0.1em !important;
        background: none !important;
        padding: 0 !important;
      }

      .wp-block-button__link {
        color: #fff;
        background-color: #32373c;
        border-radius: 9999px;
        box-shadow: none;
        text-decoration: none;
        padding: calc(0.667em + 2px) calc(1.333em + 2px);
        font-size: 1.125em;
      }

      .wp-block-file__button {
        background: #32373c;
        color: #fff;
        text-decoration: none;
      }

      :root {
        --wp--preset--color--black: #000000;
        --wp--preset--color--white: #ffffff;
        --wp--preset--font-size--small: 18px;
        --wp--preset--font-size--medium: 20px;
        --wp--preset--font-size--large: 24px;
        --wp--preset--font-size--x-large: 42px;
      }

      :where(.is-layout-flex) {
        gap: 0.5em;
      }

      body .is-layout-flex {
        display: flex;
      }

      .is-layout-flex {
        flex-wrap: wrap;
        align-items: center;
      }

      .is-layout-flex > :is(*, div) {
        margin: 0;
      }

      #tss-container-109931952.tss-wrapper .tss-layout10 .item-content-wrapper,
      #tss-container-109931952.tss-wrapper .tss-isotope3 .item-content-wrapper,
      #tss-container-109931952.tss-wrapper .tss-isotope5 .item-content-wrapper,
      #tss-container-109931952.tss-wrapper
        .tss-carousel10
        .item-content-wrapper {
        background: transparent;
      }

      .wpb_animate_when_almost_visible {
        opacity: 1;
      }
    </style>
  </head>

  <body
    class="home page-template page-template-home-page page-template-home-page-php page page-id-27 wp-custom-logo wp-embed-responsive is-light-theme no-js singular has-main-navigation no-widgets wpb-js-composer js-comp-ver-7.6 vc_responsive"
  >
    <div id="page" class="site">
      <!-- Header -->
      <header
        id="masthead"
        class="site-header has-logo has-title-and-tagline has-menu"
      >
        <div class="container">
          <div class="top-strip d-flex flex-wrap jsb aic" style="display: flex;flex-wrap: nowrap;">
            <div class="site-logo">
              <span class="custom-logo-link">
                <img
                  width="265"
                  height="142"
                  src={logoImg.src}
                  class="custom-logo"
                  alt=""
                  decoding="async"
                />
              </span>
            </div>

            <div class="site-branding">
              <h1 class="site-title">{title}</h1>
            </div>

            <nav
              id="site-navigation"
              class="primary-navigation"
              aria-label="Primary menu"
            >
              <div class="menu-button-container">
                <button
                  id="primary-mobile-menu"
                  class="button"
                  aria-controls="primary-menu-list"
                  aria-expanded="false"
                >
                  <span class="dropdown-icon open">Menu
                    <svg
                      class="svg-icon"
                      width="24"
                      height="24"
                      aria-hidden="true"
                      role="img"
                      focusable="false"
                      viewBox="0 0 24 24"
                      fill="none"
                      xmlns="http://www.w3.org/2000/svg"
                    >
                      <path
                        fill-rule="evenodd"
                        clip-rule="evenodd"
                        d="M4.5 6H19.5V7.5H4.5V6ZM4.5 12H19.5V13.5H4.5V12ZM19.5 18H4.5V19.5H19.5V18Z"
                        fill="currentColor"
                      ></path>
                    </svg>
                  </span>
                  <span class="dropdown-icon close">Close
                    <svg
                      class="svg-icon"
                      width="24"
                      height="24"
                      aria-hidden="true"
                      role="img"
                      focusable="false"
                      viewBox="0 0 24 24"
                      fill="none"
                      xmlns="http://www.w3.org/2000/svg"
                    >
                      <path
                        fill-rule="evenodd"
                        clip-rule="evenodd"
                        d="M12 10.9394L5.53033 4.46973L4.46967 5.53039L10.9393 12.0001L4.46967 18.4697L5.53033 19.5304L12 13.0607L18.4697 19.5304L19.5303 18.4697L13.0607 12.0001L19.5303 5.53039L18.4697 4.46973L12 10.9394Z"
                        fill="currentColor"
                      ></path>
                    </svg>
                  </span>
                </button>
              </div>
            </nav>
          </div>
        </div>
      </header>

      <!-- Main Content -->
      <div id="content" class="site-content">
        <div id="primary" class="content-area">
          <main id="main" class="site-main">
            <!-- Hero Section -->
            <section class="hero-sec">
              <div class="container">
                <div class="hero-inner">
                  <section id="block-3" class="widget widget_block">
                    <div class="text-sec">
                      <h1 class="heading">
                        The "Secret" Stock Alert Group taking Wall Street by <span
                          class="clr-or"
                                                                             >Storm!</span>
                      </h1>
                      <p class="prgr">
                        Don't be fooled by those overpriced "AI" subscriptions
                        and empty promises!
                      </p>
                      <p class="prgr sec">
                        Join us now to receive real-time, exclusive stock alerts
                        – completely free.
                      </p>
                      <p class="prgr sec">
                        Seize this perfect opportunity to break free from the
                        ordinary and take your wealth to the next level.
                      </p>
                      <p class="prgr third">
                        Life-changing gains are just one click away!
                      </p>
